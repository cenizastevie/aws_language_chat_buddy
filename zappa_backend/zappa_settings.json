{
    "dev": {
        "app_function": "app.app",
        "aws_region": "us-east-1",
        "profile_name": "default",
        "project_name": "aws-language-chat-buddy",
        "runtime": "python3.9",
        "s3_bucket": "zappa-deployments-aws-language-chat-buddy",
        "environment_variables": {
            "MODEL_ID": "amazon.titan-text-lite-v1",
            "AWS_REGION": "us-east-1",
            "SECRET_KEY": "your-dev-secret-key-change-this"
        },
        "keep_warm": false,
        "timeout_seconds": 30,
        "memory_size": 512
    },
    "production": {
        "app_function": "app.app",
        "aws_region": "us-east-1",
        "profile_name": "default",
        "project_name": "aws-language-chat-buddy",
        "runtime": "python3.9",
        "s3_bucket": "zappa-deployments-aws-language-chat-buddy-prod",
        "environment_variables": {
            "MODEL_ID": "amazon.titan-text-lite-v1",
            "AWS_REGION": "us-east-1",
            "SECRET_KEY": "your-production-secret-key-change-this"
        },
        "keep_warm": false,
        "timeout_seconds": 30,
        "memory_size": 512
    }
}
